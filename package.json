{
  "name": "ebisusbay",
  "version": "2.0.9",
  "private": true,
  "homepage": "https://app.ebisusbay.com",
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 next build",
    "codegen": "graphql-codegen --config dev/codegen.ts",
    "predev": "npm run codegen",
    "start": "next start",
    "start-web": "NODE_OPTIONS=\"--max_old_space_size=1408\" next start",
    "format": "prettier --write \"**/*.+(js|jsx|json|md|mdx|css|scss)\"",
    "contracts:compile:abi": "typechain --target ethers-v5 --out-dir src/modules/dex/packages/uniswap/src/abis/types \"./src/modules/dex/packages/uniswap/src/abis/**/*.json\"",
    "contracts:compile:v3": "typechain --target ethers-v5 --out-dir src/modules/dex/packages/uniswap/src/abis/types/v3 \"../../node_modules/@uniswap/**/artifacts/contracts/**/*[!dbg].json\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,md,mdx}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
